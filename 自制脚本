local Players = game:GetService("Players")
local CoreGui = game:GetService("StarterGui")
local DeveloperWhitelist = {"TMD2540z"} -- å¼€å‘è€…åå•
local notificationSentCount = 0 -- ç”¨äºè®°å½•é€šçŸ¥å‘é€æ¬¡æ•°

-- å‘é€é€šçŸ¥çš„å‡½æ•°
local function SendNotification(title, text, duration)
    CoreGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
    })
end

-- æ£€æµ‹ç‰¹å®šç©å®¶åçš„å‡½æ•°
local function CheckPlayerName()
    for _, player in pairs(Players:GetPlayers()) do
        if table.find(DeveloperWhitelist, player.Name) then
            notificationSentCount = notificationSentCount + 1
            if notificationSentCount <= 2 then
                SendNotification("å¼€å‘è€…æ£€æµ‹", "æ£€æµ‹åˆ°å¼€å‘è€…åœ¨æœ¬æœåŠ¡å™¨", 5)
            end
            return -- å‘ç°åŒ¹é…åè·³å‡ºå¾ªç¯
        end
    end
end

-- è®¾ç½®ä¸€ä¸ªå®šæ—¶å™¨ï¼Œæ¯éš”ä¸€å®šæ—¶é—´æ‰§è¡Œä¸€æ¬¡æ£€æµ‹
game:GetService("RunService").Heartbeat:Connect(function()
    CheckPlayerName()
end)

-- è®¾ç½®å®šæ—¶å™¨çš„æ—¶é—´é—´éš”ï¼Œä¾‹å¦‚æ¯10ç§’æ£€æµ‹ä¸€æ¬¡
game:GetService("RunService").Heartbeat:Connect(function()
    wait(10) -- ç­‰å¾…10ç§’
end)

