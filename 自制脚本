---乐仔想偷脚本，我懒得加你，只要你觉得你父母
local Players = game:GetService("Players")
local CoreGui = game:GetService("StarterGui")
local DeveloperWhitelist = {"TMD2540z"} -- 开发者名单
local notificationSentCount = 0 -- 用于记录通知发送次数

-- 发送通知的函数
local function SendNotification(title, text, duration)
    CoreGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
    })
end

-- 检测特定玩家名的函数
local function CheckPlayerName()
    for _, player in pairs(Players:GetPlayers()) do
        if table.find(DeveloperWhitelist, player.Name) then
            notificationSentCount = notificationSentCount + 1
            if notificationSentCount <= 2 then
                SendNotification("开发者检测", "检测到开发者在本服务器", 5)
            end
            return -- 发现匹配后跳出循环
        end
    end
end

-- 设置一个定时器，每隔一定时间执行一次检测
game:GetService("RunService").Heartbeat:Connect(function()
    CheckPlayerName()
end)

-- 设置定时器的时间间隔，例如每10秒检测一次
game:GetService("RunService").Heartbeat:Connect(function()
    wait(10) -- 等待10秒
end)

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- 创建屏幕GUI元素
local LBLG = Instance.new("ScreenGui", game.CoreGui)
LBLG.Name = "LBLG"
LBLG.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
LBLG.Enabled = true

-- 创建文本标签
local LBL = Instance.new("TextLabel", LBLG)
LBL.Name = "LBL"
LBL.BackgroundColor3 = Color3.new(1, 1, 1)
LBL.BackgroundTransparency = 1
LBL.BorderColor3 = Color3.new(0, 0, 0)
LBL.Position = UDim2.new(0.75, 0, 0.010, 0)
LBL.Size = UDim2.new(0, 133, 0, 30)
LBL.Font = Enum.Font.GothamSemibold
LBL.Text = "TextLabel"
LBL.TextColor3 = Color3.new(1, 1, 1)
LBL.TextScaled = true
LBL.TextSize = 14
LBL.TextWrapped = true
LBL.Visible = true

-- 获取RunService并设置相关变量
local Heartbeat = game:GetService("RunService").Heartbeat
local LastIteration, Start
local FrameUpdateTable = {}

-- 心跳更新函数
local function HeartbeatUpdate()
    LastIteration = tick()
    for Index = #FrameUpdateTable, 1, -1 do
        FrameUpdateTable[Index + 1] = (FrameUpdateTable[Index] >= LastIteration - 1) and FrameUpdateTable[Index] or nil
    end
    FrameUpdateTable[1] = LastIteration
    local CurrentFPS = (tick() - Start >= 1 and #FrameUpdateTable) or (#FrameUpdateTable / (tick() - Start))
    CurrentFPS = math.floor(CurrentFPS)
    -- 设置文本为北京时间格式
    LBL.Text = ("北京时间: " .. os.date("%H") .. " 时 " .. os.date("%M") .. " 分 " .. os.date("%S") .. " 秒")
end

-- 开始计时
Start = tick()

-- 连接Heartbeat事件
Heartbeat:Connect(HeartbeatUpdate)

