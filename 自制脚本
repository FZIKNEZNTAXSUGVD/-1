local CoreGui = game:GetService("StarterGui")

-- å®šä¹‰ä¸¤ä¸ªç™½åå•
local BetaTesterWhitelist = {"wzznDdm","yfttddttr3e"} -- å†…æµ‹äººå‘˜åå•
local DeveloperWhitelist = {"TMD2540z", "TMD2540"} -- å¼€å‘è€…åå•

-- å‘é€é€šçŸ¥çš„å‡½æ•°
local function SendNotification(title, text, duration)
    CoreGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
    })
end

-- æ£€æµ‹ç©å®¶èº«ä»½å¹¶å‘é€é€šçŸ¥
local function CheckPlayerIdentity()
    -- è·å–å½“å‰ç©å®¶çš„åå­—
    local playerName = game.Players.LocalPlayer.Name

    -- å‘é€åˆå§‹é€šçŸ¥
    SendNotification("è„šæœ¬å…¨è¢«æˆ‘åƒäº†ğŸ˜‹", "æ­£åœ¨æ£€æµ‹æ‚¨çš„èº«ä»½ï¼Œè¯·ç¨å€™...", 5)

    -- æ¨¡æ‹Ÿæ£€æµ‹è¿‡ç¨‹
    wait(2) -- ç­‰å¾…1ç§’é’Ÿ

    -- æ£€æµ‹ç©å®¶ç”¨æˆ·åæ˜¯å¦åœ¨ç™½åå•ä¸­
    local isBetaTester = table.find(BetaTesterWhitelist, playerName) ~= nil
    local isDeveloper = table.find(DeveloperWhitelist, playerName) ~= nil

    -- æ ¹æ®ç©å®¶èº«ä»½å‘é€é€šçŸ¥
    if isBetaTester then
        SendNotification("èº«ä»½éªŒè¯", "æ‚¨æ˜¯å†…æµ‹äººå‘˜", 5)
    elseif isDeveloper then
        SendNotification("èº«ä»½éªŒè¯", "æ‚¨æ˜¯å¼€å‘è€…", 5)
    else
        SendNotification("èº«ä»½éªŒè¯", "æ‚¨æ˜¯æˆå‘˜", 5)

wait(2)

       end
end

-- åªæ‰§è¡Œä¸€æ¬¡æ£€æµ‹
CheckPlayerIdentity()

